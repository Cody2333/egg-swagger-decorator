{
  "name": "egg-swagger-decorator",
  "version": "0.0.5",
  "description": "using decorator to make router definitions and automatically generate swagger doc for egg.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-showcase",
    "stop": "egg-scripts stop --title=egg-server-showcase",
    "dev": "egg-bin dev -r egg-ts-helper/register",
    "debug": "egg-bin debug -r egg-ts-helper/register",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint .",
    "clean": "ets clean",
    "build": "tsc -p tsconfig_publish.json",
    "pub": "tsc -p tsconfig_publish.json && npm publish"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.106",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.6.3",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.17.0",
    "egg-ts-helper": "^1.4.2",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "typescript": "^2.8.1",
    "egg": "^2.6.0",
    "egg-scripts": "^2.6.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": "https://github.com/Cody2333/koa-swagger-decorator",
  "eslintIgnore": [
    "coverage"
  ],
  "keywords": [
    "decorator",
    "swagger",
    "egg"
  ],
  "author": "Cody",
  "license": "MIT"
}
